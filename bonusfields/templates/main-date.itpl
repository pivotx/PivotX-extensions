%header%
<tr class="%showif%">
	<td width="136">
		<label><strong>%name%:</strong></label>
	</td>
	<td colspan="2" width="500">
		<input name="deferred[%fieldkey%]" type="text" class='date-picker date-picker-deferred input' id="deferred-%fieldkey%" value="" size="15" style="width: 100px;" />
		<input name="extrafields[%fieldkey%]" type="hidden" class="date-picker-extrafield" id="extrafield-%fieldkey%" value="%%fieldkey%%" size="15" />
	</td>
</tr>
%footer%
